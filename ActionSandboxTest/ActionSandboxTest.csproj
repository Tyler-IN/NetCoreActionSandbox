<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <IsPackable>true</IsPackable>
        <Title>ActionSandboxTest</Title>
        <Authors>Tyler Young</Authors>
        <Description>Action Sandbox Test!</Description>
        <Copyright>Copyright (C) 2020 Tyler Young</Copyright>
        <PackageProjectUrl>https://github.com/Tyler-IN/NetCoreActionSandbox</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Tyler-IN/NetCoreActionSandbox.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>github actions</PackageTags>
        <AssemblyVersion>1.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
        <LicenseExpression>MIT</LicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="nunit" Version="3.12.0" />
        <PackageReference Include="NUnit3TestAdapter" Version="3.15.1" />
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.4.0" />
    </ItemGroup>

</Project>
